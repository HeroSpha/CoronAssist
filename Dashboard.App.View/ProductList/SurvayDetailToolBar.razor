<div class="products-toolbar-root">
  <div class="products-toolbar-row">
    <h3>@Title</h3>
    <span class="products-toolbar-spacer" />
    <Button Color="@Color.Primary"
            Variant="@ButtonVariant.Contained" OnClick="@OpenDialog">
      <AddIcon />
    </Button>
    <Button Color="@Color.Secondary"
            Variant="@ButtonVariant.Contained" OnClick="@DeleteSurvey">
      <DeleteIcon />
    </Button>
  </div>
  
</div>
